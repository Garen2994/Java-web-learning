package garen.day01.demo;

import java.util.Arrays;

public class day01 {
	public static void main(String[] args) {
		
		int a[] = new int[100];
		for (int i = 0; i < a.length; i++) 
			a[i] = i;
		for (int i = 0; i < a.length; i++) 
			System.out.println(a[i]);
		for (int element : a)
			System.out.println(element);
		System.out.println(Arrays.toString(a));
		
	}
}
